int main()
{
    int n,m,d,i,ogl=0,c,s=0,x,z;
    cin>>x;
    for(z=1;z<=x;z++)
    {
    cin>>n;
    c=n;
    while(c>0)
    {
        ogl=ogl*10+c%10;
        c=c/10;
    }
    for(i=1;i*i<ogl;i++)
    {
        if(ogl%i==0)
        {
            s+=i;
            s+=ogl/i;
        }
    }
    s+=n+1;
    if(i*i==ogl)
    {
        s+=i;
    }
        
    }
    cout<<s;
    return 0;
}

